{
  "manifest_version": 2,
  "name": "password generator",
  "version": "0.1",
  "description": "password generator",
  "icons": {
    "48": "letter.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js", "eventPage.js"],
    "persistent": false
  },
  "permissions": ["contextMenus", "activeTab", "tabs"],

  "browser_action": {
    "default_icon": "letter.png",
    "default_popup": "main.html"
  }
}
